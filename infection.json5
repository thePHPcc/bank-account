{
  "$schema": "https://raw.githubusercontent.com/infection/infection/0.31.1/resources/schema.json",
  "phpUnit": {
    "customPath": "tools/phpunit"
  },
  "staticAnalysisTool": "phpstan",
  "phpStan": {
    "customPath": "tools/phpstan"
  },
  "threads": "max",
  "source": {
    "directories": [
      "src"
    ]
  },
  "mutators": {
    "@default": true,
    "@regex": {
      "ignore": [
        "example\\framework\\library\\Uuid::ensureIsValidUuid"
      ]
    },
    "DecrementInteger": {
      "ignore": [
        "example\\framework\\event\\ReadEventsStatement::execute"
      ]
    },
    "IncrementInteger": {
      "ignore": [
        "example\\framework\\event\\ReadEventsStatement::execute"
      ]
    },
    "ProtectedVisibility": {
      "ignore": [
        "example\\framework\\http\\GetRequest::__construct",
        "example\\framework\\http\\PostRequest::__construct"
      ]
    }
  },
  "minMsi": 100,
  "minCoveredMsi": 100
}
